    "typescript",
    "swagger",
    "mongoose"
  ],
  "author": "@johadtoledo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JohadToledo/code-verifier-backend/issues"
  },
  "homepage": "https://github.com/JohadToledo/code-verifier-backend#readme",
  "dependencies": {
    "dotenv": "^16.0.1",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "concurrently": "^7.2.2",
    "eslint": "^8.19.0",
    "eslint-config-standard": "^17.0.0",
